---
export interface Props {
  content: string;
}

const { content } = Astro.props;
---

<span
  class="inline-block py-1.5 px-2.5 leading-none text-center text-sm whitespace-nowrap align-baseline font-semibold border border-slate-500 text-slate-600 dark:text-slate-400 rounded"
  >{content}</span
>
